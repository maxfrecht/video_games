{{ form_start(form) }}
    <div class="form-group">
        {{ form_widget(form.title, {
            'attr': {'placeholder': 'placeholder'}
        }) }}
        {{ form_label(form.title, 'Titre') }}
    </div>
    <div class="form-group">
        {{ form_widget(form.pathLogo, {
            'attr': {'placeholder': 'placeholder'}
        }) }}
        {{ form_label(form.pathLogo, 'Url du logo') }}
    </div>
    <div class="form-group">
        {{ form_widget(form.forum, {
            'attr': {'placeholder': 'placeholder'}
        }) }}
        {{ form_label(form.forum, 'Url du logo') }}
    </div>
{{ form_row(form.submit, {
    'label': 'Confirmer',
    'attr': {'class': 'btn'}
}) }}
{{ form_end(form) }}
